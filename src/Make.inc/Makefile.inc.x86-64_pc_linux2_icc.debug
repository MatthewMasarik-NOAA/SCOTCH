EXE	=
LIB	= .a
OBJ	= .o

MAKE	= make
AR	= ar
ARFLAGS	= -ruv
CAT	= cat
CCS	= icc -O0 -fp-model strict -g -O0 -traceback -check-uninit -fp-stack-check
CCP     = icpc
CCD	= icpc
CFLAGS	= -DCOMMON_FILE_COMPRESS_GZ -DCOMMON_PTHREAD -DSCOTCH_DEBUG_ALL -DSCOTCH_DETERMINISTIC -DCOMMON_RANDOM_FIXED_SEED -DSCOTCH_RENAME
LDFLAGS	= -g -lz -lm -lrt
CP	= cp
LEX	= flex
LN	= ln
MKDIR	= mkdir
MV	= mv
RANLIB	= ranlib
YACC	= bison -y
